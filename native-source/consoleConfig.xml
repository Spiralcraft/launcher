<launch4jConfig>
  <!-- Required -->
  <headerType>console
  </headerType>
  
  <!-- Required -->
  <outfile>spiralcraft
  </outfile>

  <!-- Jar to wrap or reference -->
  <jar>spiralcraft.jar
  </jar>
  
  <dontWrapJar>true
  </dontWrapJar>
  
  <errTitle>Error starting Spiralcraft
  </errTitle>
  
  <downloadUrl>http://java.com/download
  </downloadUrl>
  
  <supportUrl>http://www.spiralcraft.com
  </supportUrl>
  
  <!-- 
  <cmdLine>text
  </cmdLine>
  -->
  
  <!--  startup loc relative to exe -->
  <chdir>.
  </chdir>
  
  <priority>normal
  </priority>
  
  <customProcName>true
  </customProcName>
  
  <stayAlive>true
  </stayAlive>
  
  <icon>favicon.ico
  </icon>
  
